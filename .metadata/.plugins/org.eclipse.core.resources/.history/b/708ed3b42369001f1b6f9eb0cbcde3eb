package com.in28minutes.learn_spring_framework.game;

public class PacManGame {
	public void up() {
		System.out.println("Up");
	}
	
	public void down() {
		System.out.println("Go down");
	}
	
	public void left() {
		System.out.println("Go left");
	}
	
	public void right() {
		System.out.println("Only further!");
	}


}
